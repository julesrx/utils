{
    "name": "@julesrx/utils",
    "description": "Components and helpers that I use in most of my projects",
    "keywords": [
        "typescript"
    ],
    "private": false,
    "version": "0.0.8",
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/index.mjs",
            "module": "./dist/index.esm.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist/**/*.{js,d.ts,mjs,d.mts}"
    ],
    "author": "Jules Raffoux (https://julesrx.fr)",
    "repository": "github:julesrx/utils",
    "homepage": "https://github.com/julesrx/utils",
    "license": "MIT",
    "packageManager": "pnpm@8.10.5",
    "scripts": {
        "build": "bunchee",
        "test": "vitest",
        "prepublishOnly": "pnpm run test --run && pnpm run build"
    },
    "dependencies": {
        "idb-keyval": "^6.2.1"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.2",
        "bunchee": "^3.9.2",
        "typescript": "^5.2.2",
        "vitest": "^0.34.6"
    },
    "peerDependencies": {
        "typescript": "^5.0.0",
        "unstorage": "^1.10.1"
    }
}
