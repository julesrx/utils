{
    "name": "@julesrx/utils",
    "private": false,
    "type": "module",
    "version": "0.0.12",
    "description": "Components and helpers that I use in most of my projects",
    "keywords": [
        "typescript"
    ],
    "exports": {
        "./package.json": "./package.json",
        "./cache": {
            "types": "./dist/cache.d.ts",
            "import": "./dist/cache.mjs",
            "require": "./dist/cache.cjs"
        },
        "./dates": {
            "types": "./dist/dates.d.ts",
            "import": "./dist/dates.mjs",
            "require": "./dist/dates.cjs"
        },
        "./theme": {
            "types": "./dist/theme.d.ts",
            "import": "./dist/theme.mjs",
            "require": "./dist/theme.cjs"
        }
    },
    "files": [
        "dist/**/*.{js,d.ts,mjs,d.mts}"
    ],
    "author": "Jules Raffoux (https://julesrx.fr)",
    "repository": "github:julesrx/utils",
    "homepage": "https://github.com/julesrx/utils",
    "license": "MIT",
    "packageManager": "pnpm@8.10.5",
    "scripts": {
        "build": "unbuild",
        "test": "vitest",
        "release": "release-it",
        "prepublishOnly": "pnpm run test --run && pnpm run build"
    },
    "dependencies": {
        "idb-keyval": "^6.2.1",
        "unstorage": "^1.10.1"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.3",
        "release-it": "^17.0.1",
        "typescript": "^5.3.3",
        "unbuild": "^2.0.0",
        "vitest": "^1.1.0"
    }
}
